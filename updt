# termux-url-opener
mkdir ~/bin
rm -r /data/data/com.termux/files/home/bin/termux-url-opener
wget https://raw.githubusercontent.com/vntuhu/ytbdl/main/termux-url-opener -P /data/data/com.termux/files/home/bin
chmod +x /data/data/com.termux/files/home/bin/termux-url-opener

# ytbdl config
rm -r /data/data/com.termux/files/home/.config/youtube-dl/config
wget https://raw.githubusercontent.com/vntuhu/ytbdl/main/config -P /data/data/com.termux/files/home/.config/youtube-dl
mkdir /data/data/com.termux/files/home/storage/shared/Youtube
mkdir /data/data/com.termux/files/home/storage/shared/Music/
mkdir /data/data/com.termux/files/home/storage/shared/Movies/

# ytbdl short
rm -r /data/data/com.termux/files/usr/bin/ytb
wget https://raw.githubusercontent.com/vntuhu/ytbdl/main/ytb -P /data/data/com.termux/files/usr/bin/
chmod +x /data/data/com.termux/files/usr/bin/ytb

# video converter
rm -r /data/data/com.termux/files/usr/bin/conv
wget https://raw.githubusercontent.com/vntuhu/converter/main/conv -P /data/data/com.termux/files/usr/bin
chmod +x /data/data/com.termux/files/usr/bin/conv
mkdir /sdcard/conv
touch /sdcard/conv/this-is-conv-folder

# ytbdl option
rm -r /data/data/com.termux/files/usr/bin/yto
wget https://raw.githubusercontent.com/vntuhu/ytbdl/main/yto -P /data/data/com.termux/files/usr/bin
chmod +x /data/data/com.termux/files/usr/bin/yto

echo "Done..."
